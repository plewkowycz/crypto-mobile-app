{
  "name": "AwesomeApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:maestro:settings": "maestro test maestro/flows/settings/settings_flow.yaml --shard-all 2 --test-output-dir ./test-results/settings/screenshots --format html --output ./test-results/settings/report.html",
    "test:maestro:dashboard": "maestro test maestro/flows/dashboard/dashboard_flow.yaml --shard-all 2 --test-output-dir ./test-results/dashboard/screenshots --format html --output ./test-results/dashboard/report.html",
    "test:maestro:sidemenu": "maestro test maestro/flows/sidemenu/sidemenu_flow.yaml --shard-all 2 --test-output-dir ./test-results/sidemenu/screenshots --format html --output ./test-results/sidemenu/report.html",
    "test:maestro:integration": "maestro test maestro/flows/integration/integration_flow.yaml --shard-all 2 --test-output-dir ./test-results/integration/screenshots --format html --output ./test-results/integration/report.html"
  },
  "dependencies": {
    "@react-native-community/checkbox": "^0.5.20",
    "@react-native-vector-icons/feather": "^12.3.0",
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/bottom-tabs": "^7.4.5",
    "@react-navigation/drawer": "^7.5.6",
    "@react-navigation/native": "^7.1.17",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-reanimated": "^4.0.1",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.13.1",
    "react-native-simple-markdown": "^1.1.0",
    "react-native-worklets": "^0.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
